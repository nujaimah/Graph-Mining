        "## directed degree centrality\n",
        "def degree_centrality(g, weights = None):\n",
        "    n = g.vcount()\n",
        "    if g.is_directed():\n",
        "        dc = [\n",
        "           ## sum in degrees, out degrees of every node\n",
        "           ## divide by n - 1 = max # of neighbours node could have\n",
        "           ## divide by 2 because of in and out degrees considered\n",
        "            sum(x) / (2 * (n - 1))\n",
        "            for x in zip(\n",
        "                g.strength(mode = \"in\", weights = weights), g.strength(mode = \"out\", weights = weights)\n",
        "            )\n",
        "        ]\n",
        "    else:\n",
        "        ## undirected graph, don't divide by 2 (just sum edge weights)\n",
        "        dc = [x / (n - 1) for x in g.strength(weights = weights)]\n",
        "    return dc\n",
        "\n",
        "## use distance = number of nodes below if disconnected\n",
        "def closeness_centrality(g):\n",
        "    n = G_state.vcount()\n",
        "    D = np.array(g.distances(mode=\"all\"))\n",
        "    D[D == np.inf] = n\n",
        "    return [(n - 1) / sum(D[i]) for i in range(len(D))]\n",
        "\n",
        "## get state names of each node\n",
        "G_state.vs[\"name\"] = state_names\n",
        "\n",
        "## compute several centrality measures for the graph\n",
        "centralities_df = pd.DataFrame(\n",
        "    {\n",
        "        \"state\": G_state.vs[\"name\"],\n",
        "        \"degree\": degree_centrality(G_state),\n",
        "        \"pagerank\": G_state.pagerank(),\n",
        "        \"authority\": G_state.authority_score(),\n",
        "        \"hub\": G_state.hub_score(),\n",
        "        \"between\": G_state.betweenness(),\n",
        "        \"harmonic\": G_state.harmonic_centrality(),\n",
        "        \"closeness\": closeness_centrality(G_state),\n",
        "    }\n",
        ")\n",
        "\n",
        "## normalize the betweenness values, not normalized in igraph\n",
        "n = G_state.vcount()\n",
        "centralities_df[\"between\"] = [2 * x / ((n - 1) * (n - 2)) for x in centralities_df[\"between\"]]\n",
        "\n",
        "## sort w.r.t. degree centrality in descending order, look at top airports\n",
        "degree_sorted = centralities_df.sort_values(by = \"degree\", ascending = False)\n",
        "\n",
        "## sort w.r.t betweeness centrality in descending order, look at top airports\n",
        "betweenness_sorted = centralities_df.sort_values(by = \"between\", ascending = False)\n",
        "\n",
        "## print results for top 5 states in degree centrality\n",
        "print(\"Top states by Degree Centrality:\")\n",
        "print(degree_sorted.head())\n",
        "\n",
        "## print results for top 5 states in betweenness centrality\n",
        "print(\"\\nTop states by Betweenness Centrality:\")\n",
        "print(betweenness_sorted.head())"
      ],
