    {
      "cell_type": "code",
      "source": [
        "## create mapping dictionary of airports and their states in the graph (keys are index of each node (airport), values are states)\n",
        "airport_states = {v.index: v[\"state\"] for v in g_airport.vs}  ## assuming nodes have a \"state\" attribute\n",
        "\n",
        "## get list of unique states by extracting values from airport_states dictionary\n",
        "unique_states = sorted(set(airport_states.values()))  ## set() ensures there are only unique states, and sorted() orders them alphabetically\n",
        "## create mapping dictionary that assigns a unique index to each state\n",
        "state_mapping = {state: idx for idx, state in enumerate(unique_states)}  ## map each state to unique state\n",
        "\n",
        "## create mapping for each airport to its corresponding unique state index using state_mapping dictionary\n",
        "contract_mapping = [state_mapping[airport_states[v.index]] for v in g_airport.vs]\n",
        "\n",
        "## sum the edge weights for each state\n",
        "G_state = g_airport.copy() ## create copy of graph to preserve original data\n",
        "## use contract_vertices to add edge weights\n",
        "G_state.contract_vertices(contract_mapping, combine_attrs = dict(weight = \"sum\"))  ## merge airports into states by summing \"weight\"\n",
        "\n",
        "## remove loops (flights within the same state)\n",
        "G_state.simplify(combine_edges = sum, loops = False)  ## sum edge weights, remove self-loops\n",
        "\n",
        "## print number of nodes of new graph G\n",
        "print(\"Number of nodes in new graph G:\", G_state.vcount())\n",
        "\n",
        "## check connectivity\n",
        "is_weakly_connected = G_state.is_connected(mode = \"weak\")\n",
        "is_strongly_connected = G_state.is_connected(mode = \"strong\")\n",
        "\n",
        "## print results\n",
        "print(f\"Is the state-to-state graph weakly connected? {is_weakly_connected}\")\n",
        "print(f\"Is the state-to-state graph strongly connected? {is_strongly_connected}\")"
