      "source": [
        "## compute the normalized edge weights\n",
        "max_weight = np.max(g_airport.es[\"weight\"])\n",
        "g_airport.es()[\"normalized_weight\"] = [w / max_weight for w in g_airport.es()[\"weight\"]]\n",
        "\n",
        "## directed degree centrality\n",
        "def degree_centrality(g, weights=None):\n",
        "    n = g.vcount()\n",
        "    if g.is_directed():\n",
        "        dc = [\n",
        "            sum(x) / (2 * (n - 1))\n",
        "            for x in zip(\n",
        "                g.strength(mode=\"in\", weights=weights), g.strength(mode=\"out\", weights=weights)\n",
        "            )\n",
        "        ]\n",
        "    else:\n",
        "        dc = [x / (n - 1) for x in g.strength(weights=weights)]\n",
        "    return dc\n",
        "\n",
        "## compute centrality measures\n",
        "centralities = {\n",
        "    \"degree\": degree_centrality(g_airport, weights=\"normalized_weight\"),\n",
        "    \"pagerank\": g_airport.pagerank(weights=\"weight\"),\n",
        "    \"authority\": g_airport.authority_score(weights=\"weight\"),\n",
        "    \"hub\": g_airport.hub_score(weights=\"weight\"),\n",
        "    \"between\": g_airport.betweenness(),\n",
        "    \"harmonic\": g_airport.harmonic_centrality(),\n",
        "}\n",
        "\n",
        "## function to check when weak connectivity breaks\n",
        "def check_disconnection(graph, sorted_nodes):\n",
        "    g_copy = graph.copy()  ## work on a fresh copy of the graph\n",
        "    initial_components = len(g_copy.connected_components(mode=\"weak\"))  ## initial component count\n",
        "\n",
        "    for i in range(len(sorted_nodes)):\n",
        "        g_copy = graph.copy()  ## ensure a fresh graph at every step\n",
        "        g_copy.delete_vertices(sorted_nodes[: i + 1])  ## remove the first (i+1) nodes\n",
        "        new_components = len(g_copy.connected_components(mode=\"weak\"))  ## count components\n",
        "\n",
        "        if new_components > initial_components:  ## graph is no longer weakly connected\n",
        "            return i + 1  ## number of nodes removed to disconnect\n",
        "    return len(sorted_nodes)  ## if still connected, return total nodes removed\n",
        "\n",
        "## store results\n",
        "results = []\n",
        "\n",
        "## experiment with largest and smallest centrality scores\n",
        "for measure, values in centralities.items():\n",
        "    g_copy = g_airport.copy()  ## ensure each experiment uses a fresh graph\n",
        "\n",
        "    ## sort nodes by centrality scores (to avoid repeated lowest values)\n",
        "    sorted_high = sorted(range(g_copy.vcount()), key=lambda x: values[x], reverse=True)  ## descending\n",
        "    sorted_low = sorted(range(g_copy.vcount()), key=lambda x: (values[x], g_copy.degree(x)))  ## ascending, tie-break by degree\n",
        "\n",
        "    ## compute number of nodes to remove for disconnection\n",
        "    high_removals = check_disconnection(g_airport, sorted_high)\n",
        "    low_removals = check_disconnection(g_airport, sorted_low)\n",
        "\n",
        "    ## store results in list\n",
        "    results.append([measure, high_removals, low_removals])\n",
        "\n",
        "## convert results to data frame\n",
        "df_results = pd.DataFrame(results, columns = [\"Centrality Measure\", \"Nodes Removed (Largest)\", \"Nodes Removed (Smallest)\"])\n",
        "\n",
        "## print as a formatted table\n",
        "print(df_results.to_string(index = False))"
      ],
