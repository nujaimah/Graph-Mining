      "source": [
        "## Question 2"
      ],
      "metadata": {
        "id": "k7r-T9oky-Gy"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "## For this question we are assuming an undirected graph, and assigning weights using formula 2.9"
      ],
      "metadata": {
        "id": "MKUSmK5Mkgx-"
      },
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "## fast Chung-Lu: generate m distinct edges w.r.t. distribution d, no loops\n",
        "def fast_CL(d, m, seed = 123): ## m edges, degree distribution d,\n",
        "    np.random.seed(seed)\n",
        "    n = len(d)  ## number of nodes is length of degree distribution\n",
        "    s = np.sum(d) ## sum of all degrees\n",
        "    p = [i / s for i in d]  ## we draw nodes w.r.t. degrees, probabilities of picking a given vertex (proportional to degree), divide degree by sum of degrees\n",
        "    target = m  ## number of edges to generate\n",
        "    tples = []  ## list of generated edges\n",
        "    ## generate edges (tuples), drop collisions, until m edges are obtained.\n",
        "    while len(tples) < target:\n",
        "        s = target - len(tples)  ## number left to generate\n",
        "        e0 = np.random.choice(n, size = s, replace = True, p = p) ## pick random node\n",
        "        e1 = np.random.choice(n, size = s, replace = True, p = p) ## pick random node\n",
        "        tples.extend( ## extend list of tuples, take minimum and maximum e0 and e1 (undirected graph purposes), no same values to avoid loops\n",
        "            [(min(e0[i], e1[i]), max(e0[i], e1[i])) for i in range(len(e0)) if e0[i] != e1[i]]\n",
        "        )  ## ignore loops\n",
        "        tples = list(set(tples))  ## drop duplicates, use set to make everything unique, pass it back to list\n",
        "    return tples"
      ],
      "metadata": {
        "id": "ulFrly0UvOio"
      },
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# np.random.seed(23)\n",
        "gamma = 2.5\n",
        "n = 10000\n",
        "\n",
        "## min and max degrees\n",
        "delta = 1\n",
        "Delta = 100\n",
        "\n",
        "## generate degrees, power law degree distribution using formula (2.9): w_i =  * (n/(i-1+n/( / )^( - 1)))^(1/(-1))\n",
        "weights = []\n",
        "for i in np.arange(1, n + 1):\n",
        "    weights.append(delta * (n / (i - 1 + n / (Delta / delta) ** (gamma - 1))) ** (1 / (gamma - 1)))\n",
        "deg = [int(np.round(w)) for w in weights] ## to enforce integer weights\n",
        "## deg = weights\n",
        "\n",
        "## generate graph with Chung-Lu model\n",
        "m = int(np.mean(deg) * n / 2) ## number of edges we want\n",
        "tpl = fast_CL(deg, m, seed = 23) ## generating all edges, list of tuples\n",
        "g_pl = ig.Graph.TupleList(tpl) ## pass list of tuples to tuple list graph\n",
        "## edge_weights = [deg[e[0]] + deg[e[1]] for e in tpl] ## sum of degrees of two nodes\n",
        "## g_pl.es[\"weight\"] = edge_weights ## assigning weights to edges"
      ],
      "metadata": {
        "id": "20X6ma_RwK6O"
      },
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "## directed degree centrality - we are only really using the undirected degree centrality formula here - dc = [x / (n - 1) for x in g.strength(weights=weights)]\n",
        "def degree_centrality(g, weights = None):\n",
        "    n = g.vcount()\n",
        "    if g.is_directed():\n",
        "        dc = [\n",
        "            sum(x) / (2 * (n - 1))\n",
        "            for x in zip(\n",
        "                g.strength(mode=\"in\", weights=weights), g.strength(mode=\"out\", weights=weights)\n",
        "            )\n",
        "        ]\n",
        "    else:\n",
        "        dc = [x / (n - 1) for x in g.strength(weights=weights)]\n",
        "    return dc\n",
        "\n",
        "## use distance = number of nodes below if disconnected\n",
        "def closeness_centrality(g):\n",
        "    n = g.vcount()\n",
        "    D = np.array(g.distances(mode=\"all\"))\n",
        "    D[D == np.inf] = n\n",
        "    return [(n - 1) / sum(D[i]) for i in range(len(D))]\n",
        "\n",
        "## compute centrality measures\n",
        "df_centralities = pd.DataFrame(\n",
        "    {\n",
        "    \"degree\": degree_centrality(g_pl),\n",
        "    \"pagerank\": g_pl.pagerank(),\n",
        "    \"authority\": g_pl.authority_score(),\n",
        "    \"hub\": g_pl.hub_score(),\n",
        "    \"between\": g_pl.betweenness(),\n",
        "    \"closeness\": closeness_centrality(g_pl),\n",
        "    }\n",
        ")\n",
        "\n",
        "## normalize the betweenness values, not normalized in igraph\n",
        "n = g_pl.vcount()\n",
        "df_centralities[\"between\"] = [2 * x / ((n - 1) * (n - 2)) for x in df_centralities[\"between\"]]\n",
        "\n",
        "## get all nodes of degree k\n",
        "deg = []\n",
        "deg = g_pl.degree()\n",
        "f_k = df_centralities.groupby(deg).mean()\n",
        "\n",
        "print(df_centralities.columns) ## check all centralities are included\n",
        "print(f_k.head())  ## check if f_k is valid\n",
        "\n",
        "## plot f(k) vs. k\n",
        "plt.figure(figsize = (12, 8))\n",
        "for measure in [\"degree\", \"pagerank\", \"authority\", \"hub\", \"between\", \"closeness\"]:\n",
        "    plt.scatter(f_k.index, f_k[measure], label = measure)\n",
        "\n",
        "## plot on log-log scale\n",
        "plt.xscale(\"log\")\n",
        "plt.yscale(\"log\")\n",
        "plt.xlabel(\"Degree k\")\n",
        "plt.ylabel(\"Average Centrality f(k)\")\n",
        "plt.legend()\n",
        "plt.title(\"Average Centrality Measure over all nodes of Degree k\")\n",
        "plt.show()"
      ],
